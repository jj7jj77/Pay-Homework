        System.out.print("What is your skill level?");
        int skill = imp.nextInt();
        
        System.out.print("How many hours did you work this week??");
        int hoursWorked = imp.nextInt();
        
         //= payrate  * skill;
        if (skill==1)
        {
            payRate_1 = 17;
        }
        elseif (skill==2)
        {
            payRate_2 =20;
        }
        elseif (skill==3)
        {
            payRate_3 =22;
        }
        
   
        //String medicalInsurance;
